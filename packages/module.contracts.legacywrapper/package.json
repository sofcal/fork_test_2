  "publishConfig": {
    "registry": "https://sage.myget.org/F/npm/npm/",
    "access": "public"
  },
  "dependencies": {
    "@sage/bc-adapterresources": "^1.1.23",
    "@sage/bc-alerttypes": "^1.1.22",
    "@sage/bc-bank": "^1.1.119",
    "@sage/bc-bankids": "^1.1.19",
    "@sage/bc-common-helpers": "^1.1.119",
    "@sage/bc-common-resources": "^1.1.23",
    "@sage/bc-common-statuscodeerror": "^1.1.119",
    "@sage/bc-contracts-quarantine": "^1.1.119",
    "@sage/bc-contracts-transaction": "^1.1.119",
    "@sage/bc-lock": "^1.1.52",
    "@sage/bc-productids": "^1.1.23",
    "@sage/bc-rule": "^1.1.29",
    "@sage/bc-servicenames": "^1.1.23",
    "@sage/bc-services-utils": "^1.1.119",
    "@sage/bc-uncappedquery": "^1.1.52",
    "@sage/bc-validators": "^1.1.119"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-stats": "^1.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2",
    "serverless-webpack": "^4.2.0",
    "should": "^13.2.1",
    "sinon": "^5.0.10",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "buildwrapper": "npm i && babel src -d lib --copy-files && npm run clean && npm i --production",
    "clean": "rimraf ./coverage/ && rimraf ./.nyc_output/ && rimraf ./node_modules/",
    "test": "NODE_ENV=test nyc --cache false --x test/ mocha -b --colors \"./test/unit/**/*.spec.js\"",
    "testwatchunit": "NODE_ENV=test mocha -b -w --colors \"./test/unit/**/*.spec.js\"",
    "testwatchuint": "NODE_ENV=test mocha -b -w --colors \"./test/int/**/*.spec.js\"",
    "testint": "NODE_ENV=test nyc --x lib/_legacy --x test/ mocha -b --colors --reporter spec \"./test/int/**/*.spec.js\"",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "cover:report": "npm run cover && npm run report",
    "lint": "eslint ./src/**",
    "build": "",
    "watch": "babel --watch src --out-dir lib --copy-files",
    "unlock": "rimraf package-lock.json"
  }
}
