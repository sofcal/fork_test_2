{
  "name": "@sage/bc-contracts-rule",
  "version": "1.3.1",
  "description": "Banking Cloud schemas",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://sage.myget.org/F/npm/npm/",
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./coverage/ && rm -rf ./.nyc_output/ && rm -rf ./.serverless/ && rm -rf ./node_modules/",
    "test": "NODE_ENV=test nyc --cache false --x test/ mocha -b --colors \"./test/unit/**/*.js\"",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "int": "NODE_ENV=test nyc --x lib/_legacy --x test/ mocha -b --colors --reporter spec \"./test/int/**/*.js\"",
    "lint": "eslint ./src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sage/bc-jsonschema-to-statuscodeerror": "^1.3.1",
