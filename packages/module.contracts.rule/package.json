    "bignumber.js": "^4.0.0",
    "bluebird": "^3.5.1",
    "emitter": "0.0.5",
    "jsonschema": "^1.2.4",
    "safe-access": "^0.1.0",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "mongodb": "^3.0.5",
    "nock": "^9.0.13",
    "nyc": "^12.0.2",
    "should": "^13.2.1",
    "sinon": "^5.0.10",
    "sleep": "^5.1.0",
    "snyk": "^1.19.1",
    "watch": "^1.0.2"
  }
