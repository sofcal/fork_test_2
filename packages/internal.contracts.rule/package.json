{
  "name": "schemas",
  "version": "1.0.0",
  "description": "Banking Cloud schemas",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./coverage/ && rm -rf ./.nyc_output/ && rm -rf ./.serverless/ && rm -rf ./node_modules/",
    "test": "NODE_ENV=test nyc --cache false --x test/ mocha -b --colors \"./tests/units/**/*.js\"",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "int": "NODE_ENV=test nyc --x lib/_legacy --x test/ mocha -b --colors --reporter spec \"./tests/integration/**/*.js\"",
    "lint": "eslint ./src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jsonschema": "^1.1.1",
    "bluebird": "^3.5.1",
    "emitter": "0.0.5",
    "underscore": "^1.9.0",
    "safe-access": "^0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.1.0",
    "mongodb": "^2.2.24",
    "nock": "^9.0.13",
    "nyc": "^11.4.1",
    "should": "^13.2.1",
    "sinon": "^2.3.2",
    "sleep": "^5.1.0",
    "snyk": "^1.19.1",
    "watch": "^1.0.2"
  }
}
