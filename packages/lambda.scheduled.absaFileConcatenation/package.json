{
  "name": "absa-file-concatenation",
  "version": "1.3.1",
  "author": "sage.service-fabric.banking-cloud",
  "license": "ISC",
  "description": "lambda function for concatenating ABSA bank files into a single file which can be processed by Banking Cloud",
  "main": "lib/handler.js",
  "private": true,
  "dependencies": {
    "@sage/bc-parameterstore-static-loader": "^1.3.1",
    "@sage/bc-request-logger": "^1.3.1",
    "@sage/bc-services-db": "^1.3.1",
    "@sage/bc-services-s3": "^1.3.1",
    "@sage/bc-status-code-error": "^1.3.1",
    "aws-sdk": "^2.220.1",
    "bluebird": "^3.5.1",
    "moment": "^2.10.2",
    "mongodb": "^3.0.5",
    "underscore": "^1.9.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-tcomb": "^0.3.27",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-stats": "^1.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2",
    "serverless-webpack": "^4.2.0",
    "should": "^13.2.1",
    "sinon": "^5.0.10",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "clean": "rimraf ./coverage/ && rimraf ./.nyc_output/ && rimraf ./.serverless/ && rimraf ./node_modules/",
    "test": "NODE_ENV=test nyc --cache false --x test/ mocha -b --colors \"./test/unit/**/*.spec.js\"",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "int": "NODE_ENV=test nyc --x test/ mocha -b --colors --reporter spec \"./test/int/**/*.spec.js\"",
    "build": "serverless package",
    "postbuild": "mkdir -p ../../build && mv ./.serverless/${PWD##*/}.zip ../../build/${PWD##*/}.zip",
    "unlock": "rimraf package-lock.json",
    "lint": "eslint ./lib/**"
  }
}
