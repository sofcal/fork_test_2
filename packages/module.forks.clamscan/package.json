{
  "name": "@sage/bc-forks-clamscan",
  "version": "1.1.31",
  "author": "Kyle Farris <kfarris@chomponllc.com> (http://chomponllc.com)",
  "description": "Transpiled version of clamscan to remove async/await. Use Node JS to scan files on your server with ClamAV's clamscan/clamdscan binary or via TCP to a remote server or local UNIX Domain socket. This is especially useful for scanning uploaded files provided by un-trusted sources.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "contributors": [
    "dietervds",
    "nicolaspeixoto",
    "urg <Patrick McAndrew>",
    "SaltwaterC <È˜tefan Rusu>"
  ],
  "scripts": {
    "test": "make test",
    "clean": "rimraf ./coverage/ && rimraf ./.nyc_output/ && rimraf ./node_modules/ && rimraf ./lib/",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "cover:report": "npm run cover && npm run report",
    "int": "NODE_ENV=test nyc --x lib/_legacy --x test/ mocha -b --colors --reporter spec \"./test/int/**/*.spec.js\"",
    "lint": "eslint ./src/**",
    "build": "babel --root-mode upward src -d lib --copy-files",
    "watch": "babel --root-mode upward --watch src --out-dir lib --copy-files",
    "unlock": "rimraf package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kylefarris/clamscan.git"
  },
  "keywords": [
    "clamav",
    "virus",
    "clamscan",
    "upload",
    "virus scanning",
    "clam",
    "clamd",
    "security"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/kylefarris/clamscan/issues"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "bluebird": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-transform-async-to-promises": "^0.8.12",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-stats": "^1.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2",
    "request": "^2.88.0",
    "serverless-webpack": "^4.2.0",
    "should": "^13.2.1",
    "sinon": "^5.0.10",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
