{
  "name": "bankingcloud-lsm-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "prepenv": "npm install -g npm@\"<7.0.0\" && npm install -g lerna@\"<4.0.0\" && npm install -g serverless@1.30.3 && npm install -g rimraf",
    "clean": "rimraf ./build && rimraf ./coverage && rimraf ./.nyc_output && rimraf node_modules && lerna run clean",
    "rebuild": "npm run clean && npm run build",
    "prebuild": "lerna bootstrap --hoist --no-hoist=@sage/*",
    "build": "lerna run build",
    "unlock": "rimraf package-lock.json && lerna run unlock",
    "test": "NODE_ENV=test nyc --cache false --x **/test/ --x **/packages/test/ -x **/packages/internal.*/src/* mocha -b --colors \"./test/unit/**/*.spec.js\"",
    "cover": "NODE_ENV=test npm run test && nyc report --reporter=lcov",
    "report": "open coverage/lcov-report/index.html",
    "lint": "eslint ./packages/**",
    "lint/byError": "eslint ./packages/** --format ./node_modules/eslint-stats/byError.js",
    "lint/byWarning": "eslint ./packages/** --format ./node_modules/eslint-stats/byWarning.js",
    "lint/byErrorAndWarning": "eslint ./packages/** --format ./node_modules/eslint-stats/byErrorAndWarningStacked.js",
    "lint/byFolder": "eslint ./packages/** --format ./node_modules/eslint-stats/byFolder.js",
    "release:ci": "lerna publish from-git --registry=https://sage.myget.org/F/npm/npm/ --yes",
    "release:local": "lerna publish from-git --registry=https://sage.myget.org/F/npm/npm/",
    "template": "./templates/generate.sh",
    "version": "lerna version"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/register": "^7.0.0",
    "aws-sdk": "^2.220.1",
    "babel-eslint": "^8.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-tcomb": "^0.3.27",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "bluebird": "^3.5.1",
    "emitter": "0.0.5",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-stats": "^1.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "keypair": "^1.0.1",
    "mocha": "^4.0.1",
    "mongodb": "3.0.5",
    "nyc": "^12.0.2",
    "serverless-webpack": "^4.2.0",
    "should": "^13.2.1",
    "sinon": "^5.0.10",
    "underscore": "^1.9.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
